{
  "compilerOptions": {
    "target": "es6",                               
    "module": "commonjs",                       
    "esModuleInterop": true,                      
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                   
    "outDir": "./build", 
    "rootDir" : "./src", 
    "sourceMap": true,
    "removeComments": true,
    "skipLibCheck": true     
  }
}
